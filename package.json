 "devDependencies": {
  "@babel/core": "^7.6.2",
  "@babel/runtime": "^7.6.2",
  "@react-native-community/eslint-config": "^0.0.5",
  "@storybook/addon-actions": "^5.3.19",
  "@storybook/addon-knobs": "^5.3.19",

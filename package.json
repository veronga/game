  "react-test-renderer": "16.11.0"
 },
 "jest": {
  "preset": "react-native"
 },
 "config": {
  "react-native-storybook-loader": {
   "searchDir": [
    "./src",
    "./packages"
   ],
   "pattern": "**/*.stories.js",
   "outputFile": "./storybook/storyLoader.js"
  }
 },
 "lint-staged": {
  "*.(js|jsx)": [
